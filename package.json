{
  "name": "service-app",
  "version": "1.0.0",
  "main": "dist/index.prod.js",
  "scripts": {
    "start": "node dist/index.prod.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "build:prod": "tsc && npm run clean",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:api": "ts-node src/test.ts",
    "postinstall": "npm run build",
    "db:reset": "npx prisma migrate reset",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:deploy": "npx prisma migrate deploy"
  },
  "keywords": [
    "service-provider",
    "api",
    "rest",
    "typescript",
    "express",
    "typeorm"
  ],
  "author": "",
  "license": "ISC",
  "description": "Service Provider REST API for Hojaega.pk",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/twilio": "^3.19.2",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "twilio": "^5.8.0",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/sqlite3": "^3.1.11",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
