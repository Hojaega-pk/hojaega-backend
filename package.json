{
  "name": "hojaega-backend",
  "version": "1.0.0",
  "main": "dist/index.prod.js",
  "scripts": {
    "start": "node dist/index.prod.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "build:prod": "tsc && npm run clean",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:api": "ts-node src/test.ts",
    "postinstall": "npm run build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "keywords": ["service-provider", "api", "rest", "typescript", "express", "prisma", "postgresql"],
  "author": "",
  "license": "ISC",
  "description": "Service Provider REST API for Hojaega.pk",
  "dependencies": {
    "express": "^5.1.0",
    "axios": "^1.6.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "@prisma/client": "^5.15.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/compression": "^1.7.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "rimraf": "^5.0.5",
    "prisma": "^5.15.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}

